{
    "name": "bview",
    "version": "0.0.1-alpha",
    "description": "vue ui components",
    "main": "lib/index.js",
    "scripts": {
        "start": "npm run dev",
        "dev": "node scripts/dev-server.js",
        "site": "node scripts/webpack.site.config.js",
        "test": "jest",
        "dist:common": "node scripts/sfc-transform/build-lib",
        "dist": "npm run dist:style && npm run dist:icon && npm run dist:common && npm run dist:prod-umd-dev && npm run dist:prod-umd",
        "dist:prod-umd": "webpack --config scripts/webpack.prod.config.js",
        "dist:prod-umd-dev": "webpack --config scripts/webpack.prod.dev.config.js",
