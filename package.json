    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "yarn test"
        }
    },
    "lint-staged": {
        "*.{js,vue}": [
            "prettier --write",
            "eslint --fix",
            "git add"
        ],
        "*.{html,less,json}": [
            "prettier --write",
            "git add"
        ]
    },
